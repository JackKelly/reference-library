 shorttitle = {Current Multiple Myeloma Treatment Strategies with},
 title = {Current Multiple Myeloma Treatment Strategies With Novel Agents: A European Perspective},
 url = {http://theoncologist.alphamedpress.org/content/15/1/6.abstract http://theoncologist.alphamedpress.org/content/15/1/6.full http://theoncologist.alphamedpress.org/content/15/1/6.full.pdf},
