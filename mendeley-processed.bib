  Number                   = {11},
  Pages                    = {1275 --1280},
  Url                      = {http://ieeexplore.ieee.org/ielx4/34/15773/00730561.pdf?tp=&arnumber=730561&isnumber=15773 http://ieeexplore.ieee.org/stamp/stamp.jsp?tp=&arnumber=730561},
