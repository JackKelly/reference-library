@Inproceedings{kolter_2012,
  title                    = {{Approximate Inference in Additive Factorial HMMs with Application to Energy Disaggregation}},
  author                   = {Kolter, Zico and Jaakkola, Tommi and Kolter, J Z},
  booktitle                = {Proceedings of the International Conference on Artificial Intelligence and Statistics},
