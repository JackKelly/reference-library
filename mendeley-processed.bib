 isbn = {978-1-4244-6894-2},
 issn = {1081-6011},
 pages = {305--316},
 publisher = {IEEE},
 shorttitle = {Security and Privacy (SP), 2010 IEEE Symposium on},
 title = {Outside the Closed World: On Using Machine Learning for Network Intrusion Detection},
