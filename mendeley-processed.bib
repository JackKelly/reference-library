@Article{,
  title                    = {Load Signature StudyâPart II: Disaggregation Framework, Simulation, and Applications},
  date                     = {2010-04},
  journaltitle             = {Power Delivery, IEEE Transactions on},
  doi                      = {10.1109/TPWRD.2009.2033800},
