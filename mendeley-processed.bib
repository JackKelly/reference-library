@Article{Murtagh2014,
  title                    = {{20â¶60â¶20 - Differences in Energy Behaviour and Conservation between and within Households with Electricity Monitors.}},
  author                   = {Murtagh, Niamh and Gatersleben, Birgitta and Uzzell, David},
  year                     = {2014},
  doi                      = {10.1371/journal.pone.0092019},
  issn                     = {1932-6203},
  month                    = {jan},
