  Author                   = {Kundu, A. and He, Yang and Chen, Mou-Yen},
  Date                     = {1998-11},
  Journaltitle             = {Pattern Analysis and Machine Intelligence, IEEE Transactions on},
  Doi                      = {10.1109/34.730561},
