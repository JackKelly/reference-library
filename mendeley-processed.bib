@Inproceedings{Shi2010,
  title                    = {{Thermal and power-aware task scheduling for Hadoop based storage centric datacenters}},
  author                   = {Shi, Bing and Srivastava, Ankur},
  year                     = {2010},
  doi                      = {http://doi.ieeecomputersociety.org/10.1109/GREENCOMP.2010.5598262},
