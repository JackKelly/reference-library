@InProceedings{ruzzelli2010,
  Title                    = {Real-time recognition and profiling of appliances through a single electricity sensor},
  Author                   = {Ruzzelli, Antonio G and Nicolas, C and Schoofs, Anthony and O'Hare, Gregory MP},
